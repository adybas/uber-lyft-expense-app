<h1><%= @expense_report.month %> | <%= @expense_report.year %></h1>

<br>

<h4>Expenses</h4>

<ul>
  <% @expenses.each do |expense| %>
    <li><%= expense.categories.last.name %> | $<%= expense.cost %> | <%= link_to "View", expense_path(expense) %></li>
  <% end %>
</ul>

<br>

<%= link_to "Add Expense", new_expense_report_expense_path(@expense_report) %> |
<!-- I want a link here to direct a user back to their expense reports -->
<%= link_to "Profile", user_path(@expense_report.user) %>
